#include <stdio.h>
int main() {
    int m, n, p, q;
    printf("Enter rows and columns for first matrix: ");
    scanf("%d %d", &m, &n);
    printf("Enter rows and columns for second matrix: ");
    scanf("%d %d", &p, &q);
    if (n != p) {
        printf("Matrices cannot be multiplied!\n");
        printf("Columns of first matrix must equal rows of second matrix.\n");
        return 1;
    }
    int first[m][n], second[p][q], result[m][q];
    printf("\nEnter elements of first matrix (%dx%d):\n", m, n);
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < n; j++) {
            scanf("%d", &first[i][j]);
        }
    }
    printf("\nEnter elements of second matrix (%dx%d):\n", p, q);
    for(int i = 0; i < p; i++) {
        for(int j = 0; j < q; j++) {
            scanf("%d", &second[i][j]);
        }
    }
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < q; j++) {
            result[i][j] = 0;
        }
    }
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < q; j++) {
            for(int k = 0; k < n; k++) {
                result[i][j] += first[i][k] * second[k][j];
            }
        }
    }
    printf("\nResultant matrix (%dx%d):\n", m, q);
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < q; j++) {
            printf("%d\t", result[i][j]);
        }
        printf("\n");
    }
    return 0;
}
