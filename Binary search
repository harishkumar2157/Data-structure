
#include <stdio.h>
int main(){
    int n,key;
    printf("Enter size: ");
    scanf("%d",&n);
    int a[n];
    printf("Enter sorted array:\n");
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Enter key: ");
    scanf("%d",&key);

    int l=0,r=n-1,found=0;
    while(l<=r){
        int m=(l+r)/2;
        if(a[m]==key){ printf("Found at index %d\n",m); found=1; break; }
        else if(a[m]<key) l=m+1;
        else r=m-1;
    }
    if(!found) printf("Not found\n");
    return 0;
}
