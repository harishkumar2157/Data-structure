
#include <stdio.h>

int main(){
    int n,key;
    printf("Enter size: ");
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Enter key: ");
    scanf("%d",&key);
    for(int i=0;i<n;i++){
        if(a[i]==key){ printf("Found at index %d\n",i); return 0; }
    }
    printf("Not found\n");
    return 0;
}
